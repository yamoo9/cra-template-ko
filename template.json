{
  "package": {
    "dependencies": {
      "@testing-library/jest-dom": "^5.11.9",
      "@testing-library/react": "^11.2.5",
      "@testing-library/user-event": "^12.8.1",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "@craco/craco": "^6.1.1",
      "craco-alias": "^2.2.0",
      "craco-favicons": "^1.0.0",
      "craco-image-optimizer-plugin": "^0.0.4",
      "classnames": "^2.2.6",
      "sass": "^1.32.8",
